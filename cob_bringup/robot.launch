<?xml version="1.0"?>
<launch>
  <param name="/use_sim_time" value="false"/>
  <arg name="pc1" default="localhost"/>
  <arg name="robot" default="raw3-6"/>
  <arg name="sim" default="false"/>
  <arg name="teleop" default="false"/>
  <arg name="python_urx" default="false"/>
  <arg name="ur_ip" default="192.168.1.40"/>
  <arg name="gripper_ip" default="192.168.1.11"/>
  <arg name="env-script" default="$(find cob_bringup)/env.sh"/>

  <include file="$(find cob_bringup)/robots/$(arg robot).launch">
    <arg name="env-script" value="$(arg env-script)"/>
    <arg name="pc1" value="$(arg pc1)"/>
    <arg name="robot" value="$(arg robot)"/>
    <arg name="sim" value="$(arg sim)"/>
    <arg name="teleop" value="$(arg teleop)"/>
    <arg name="python_urx" value="$(arg python_urx)"/>
    <arg name="ur_ip" value="$(arg ur_ip)"/>
    <arg name="gripper_ip" value="$(arg ur_ip)"/>
  </include>

</launch>